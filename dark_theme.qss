QWidget {font: 15px; color: #E0E0E0;}
QLineEdit {
    border: 2px solid #555555;
    border-radius: 15px;
    padding: 5px;
    background-color: #333333;
    color: #E0E0E0;
}
QLineEdit:hover {border: 2px solid #777777;}
QPushButton {
    border: 2px solid #555555;
    border-radius: 15px;
    padding: 5px;
    background-color: #444444;
    color: #E0E0E0;
    outline: none;
}
QPushButton:hover {background-color: #666666;}
QPushButton:pressed {background-color: #555555;}